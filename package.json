{
  "name": "stocks-server",
  "version": "1.0.0",
  "type": "module",
  "main": "src/jobs/tweet-job.ts",
  "scripts": {
    "tweet": "tsx src/jobs/tweet-job.ts",
    "schedule": "tsx src/scheduler.ts",
    "pattern-scan": "tsx src/jobs/pattern-scan-job.ts",
    "movers": "tsx src/jobs/movers-job.ts",
    "smart-movers": "tsx src/jobs/smart-movers-job.ts",
    "pattern-alerts": "tsx src/jobs/pattern-alerts-job.ts",
    "sector-momentum": "tsx src/jobs/sector-momentum-job.ts",
    "momentum-scanner": "tsx src/jobs/momentum-scanner-job.ts",
    "stock-picks": "tsx src/jobs/stock-picks-job.ts",
    "market-insights": "tsx src/jobs/market-insights-job.ts",
    "dashboard:build": "cd dashboard && npm run build",
    "dashboard:serve": "tsx src/dashboard-server.ts",
